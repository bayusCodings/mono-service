{
    "name": "mono-api",
    "version": "0.0.1",
    "description": "",
    "author": "Abayomi",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "prebuild": "rimraf dist",
        "preinstall": "npx npm-force-resolutions",
        "build": "nest build",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "standardx \"src/**/*.ts\"",
        "lint:fix": "standardx --fix \"src/**/*.ts\"",
        "test": "jest --config=jest.json",
        "test:watch": "jest --watch --config=jest.json",
        "test:cov": "echo 'redundant step, skipping'",
        "test:e2e": "jest --config=jest-e2e.json --verbose --coverage --coverageDirectory=coverage"
    },
    "dependencies": {
        "@nestjs/common": "7.6.5",
        "@nestjs/config": "0.5.0",
        "@nestjs/core": "7.6.5",
        "@nestjs/jwt": "^8.0.0",
        "@nestjs/microservices": "7.6.5",
        "@nestjs/mongoose": "^8.0.1",
        "@nestjs/passport": "^8.0.0",
        "@nestjs/platform-express": "7.6.5",
        "@nestjs/platform-socket.io": "^8.0.6",
        "@nestjs/swagger": "4.8.0",
        "@nestjs/terminus": "7.0.1",
        "@nestjs/throttler": "^2.0.0",
        "@nestjs/typeorm": "7.1.5",
        "@nestjs/websockets": "^8.0.6",
        "cache-manager": "3.4.3",
        "cache-manager-redis-store": "2.0.0",
        "class-transformer": "0.2.3",
        "class-validator": "0.12.2",
        "crypto-random-string": "3.2.0",
        "date-fns": "^2.23.0",
        "dotenv": "^10.0.0",
        "lodash": "^4.17.21",
        "mongoose": "^5.13.7",
        "nest-transact": "1.1.3",
        "nestjs-pino": "1.3.0",
        "passport": "^0.4.1",
        "passport-jwt": "^4.0.0",
        "redis": "3.1.2",
        "reflect-metadata": "0.1.13",
        "rimraf": "3.0.2",
        "rxjs": "6.5.4",
        "swagger-ui-express": "4.1.4",
        "typeorm": "0.2.25",
        "uniqid": "^5.3.0"
    },
    "devDependencies": {
        "@nestjs/cli": "7.5.4",
        "@nestjs/schematics": "7.2.6",
        "@nestjs/testing": "7.6.5",
        "@types/cache-manager": "2.10.3",
        "@types/chai": "4.2.11",
        "@types/express": "4.17.3",
        "@types/jest": "26.0.3",
        "@types/minimist": "1.2.0",
        "@types/node": "13.9.1",
        "@types/passport-jwt": "^3.0.6",
        "@types/supertest": "2.0.8",
        "@types/uuid": "8.3.0",
        "@typescript-eslint/eslint-plugin": "3.0.2",
        "@typescript-eslint/parser": "3.0.2",
        "chai": "4.2.0",
        "eslint": "7.1.0",
        "eslint-config-prettier": "6.10.0",
        "eslint-plugin-import": "2.20.1",
        "husky": "4.2.5",
        "jest": "26.1.0",
        "nodemon": "^2.0.12",
        "pino-pretty": "4.5.0",
        "prettier": "1.19.1",
        "standardx": "3.0.0",
        "start-server-webpack-plugin": "2.2.5",
        "supertest": "4.0.2",
        "ts-jest": "26.1.1",
        "ts-loader": "6.2.1",
        "ts-node": "8.6.2",
        "tsconfig-paths": "3.9.0",
        "typescript": "3.9.7",
        "webpack-node-externals": "2.5.2"
    },
    "resolutions": {
        "lodash": "4.17.21"
    },
    "eslintConfig": {
        "rules": {
            "no-unused-vars": "off",
            "no-useless-constructor": "off",
            "@typescript-eslint/no-unused-vars": "error",
            "no-return-assign": "off",
            "no-unused-expressions": "off",
            "semi": "off",
            "no-return-await": "off",
            "indent": [
                "error",
                2
            ]
        },
        "env": {
            "node": true,
            "jest": true
        }
    },
    "standardx": {
        "parser": "@typescript-eslint/parser",
        "plugins": [
            "@typescript-eslint/eslint-plugin"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run lint"
        }
    }
}
